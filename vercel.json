{
  "projectSettings": {
    "framework": "vite"
  },
  "buildCommand": "cd engsci-platform && npm run build",
  "outputDirectory": "engsci-platform/dist",
  "rewrites": [{ "source": "/(.*)", "destination": "/index.html" }],
  "github": {
    "silent": true
  },
  "trailingSlash": false,
  "cleanUrls": true,
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
